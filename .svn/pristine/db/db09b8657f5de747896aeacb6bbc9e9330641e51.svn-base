.logo, .breadcroumb, .product-gallery, .news_title, .news_subject, div ins{
	float: right;
}

.news_details{
	float: left !important;
}

.pull-right{
	display: none;
}

.col-md-1, .col-md-2, .col-md-3, .col-md-4, .col-md-5, .col-md-6, .col-md-7, .col-md-8, .col-md-9, .col-md-10, .col-md-11, .col-md-12 {
    float: right !important;
}

.col-sm-1, .col-sm-2, .col-sm-3, .col-sm-4, .col-sm-5, .col-sm-6, .col-sm-7, .col-sm-8, .col-sm-9, .col-sm-10, .col-sm-11, .col-sm-12 {
    float: right !important;
}

.commenterImage, .commentText, label, input.btn.btn-primary{
	float: right !important;
}

h2, h5, p, .link_class{
	clear: both;
	float: right !important;
}

.edu_head, .ach_head{
	text-align: right !important;
}

.img_detail{
	clear: both;
	float: right;
}

.para_format{
  float: right;
  padding-right: 3px;
}

.news_subject, .form-group, .info_format, ul{
	direction: rtl !important;
}

div.alert.alert-success{
	direction: rtl;
	margin: 5px;
	padding: 5px;
}